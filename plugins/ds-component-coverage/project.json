{
  "name": "ds-component-coverage",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "plugins/ds-component-coverage/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/plugins/ds-component-coverage",
        "main": "plugins/ds-component-coverage/src/index.ts",
        "tsConfig": "plugins/ds-component-coverage/tsconfig.lib.json",
        "assets": [
          "plugins/ds-component-coverage/*.md"
        ]
      }
    },
    "plugin:demo": {
      "command": "npx @code-pushup/cli collect --config=plugins/ds-component-coverage/mocks/fixtures/coverage-audit/demo/code-pushup.config.ts",
      "options": {
        "onlyPlugins": "ds-component-coverage",
        "progress": false
      }
    },
    "plugin:asset-location": {
      "command": "npx @code-pushup/cli collect --config=plugins/ds-component-coverage/mocks/fixtures/coverage-audit/asset-location/code-pushup.config.ts",
      "options": {
        "onlyPlugins": "ds-component-coverage",
        "progress": false
      }
    },
    "plugin:style-format": {
      "command": "npx @code-pushup/cli collect --config=plugins/ds-component-coverage/mocks/fixtures/coverage-audit/style-format/code-pushup.config.ts",
      "options": {
        "onlyPlugins": "ds-component-coverage",
        "progress": false
      }
    },
    "plugin:line-number": {
      "command": "npx @code-pushup/cli collect --config=plugins/ds-component-coverage/mocks/fixtures/coverage-audit/line-number/code-pushup.config.ts",
      "options": {
        "onlyPlugins": "ds-component-coverage",
        "progress": false
      }
    },
    "plugin:template-syntax": {
      "command": "npx @code-pushup/cli collect --config=plugins/ds-component-coverage/mocks/fixtures/coverage-audit/template-syntax/code-pushup.config.ts",
      "options": {
        "onlyPlugins": "ds-component-coverage",
        "progress": false
      }
    },
    "plugin-ds-quality:demo": {
      "command": "npx @code-pushup/cli collect --config=plugins/ds-component-coverage/mocks/fixtures/minimal-design-system/code-pushup.config.ts",
      "options": {
        "onlyPlugins": "ds-quality",
        "progress": false
      }
    }
  }
}
